<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.alessandro.tcc.CadastroAlunoActivity">

    <Button
        android:id="@+id/bt_cadastrar_aluno"
        android:layout_width="368dp"
        android:layout_height="wrap_content"
        android:text="Cadastrar"
        tools:layout_editor_absoluteX="8dp"
        tools:layout_editor_absoluteY="455dp" />

    <EditText
        android:id="@+id/et_nome_aluno"
        android:layout_width="368dp"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Nome"
        tools:layout_editor_absoluteX="8dp"
        tools:layout_editor_absoluteY="16dp" />

    <EditText
        android:id="@+id/et_rg_aluno"
        android:layout_width="144dp"
        android:layout_height="36dp"
        android:ems="10"
        android:inputType="text|number"
        android:text="RG"
        tools:layout_editor_absoluteX="232dp"
        tools:layout_editor_absoluteY="203dp" />

    <EditText
        android:id="@+id/et_cpf_aluno"
        android:layout_width="143dp"
        android:layout_height="36dp"
        android:ems="10"
        android:inputType="number"
        android:text="CPF"
        tools:layout_editor_absoluteX="233dp"
        tools:layout_editor_absoluteY="250dp" />

    <EditText
        android:id="@+id/et_nacionalidade_aluno"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Nacionalidade"
        tools:layout_editor_absoluteX="8dp"
        tools:layout_editor_absoluteY="159dp" />

    <EditText
        android:id="@+id/et_naturalidade_aluno"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Naturalidade"
        tools:layout_editor_absoluteX="8dp"
        tools:layout_editor_absoluteY="203dp" />

    <EditText
        android:id="@+id/et_sexo_aluno"
        android:layout_width="138dp"
        android:layout_height="37dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Sexo"
        tools:layout_editor_absoluteX="238dp"
        tools:layout_editor_absoluteY="113dp" />

    <EditText
        android:id="@+id/et_matricula_aluno"
        android:layout_width="222dp"
        android:layout_height="37dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Matrícula"
        tools:layout_editor_absoluteX="8dp"
        tools:layout_editor_absoluteY="113dp" />

    <EditText
        android:id="@+id/et_turma_aluno"
        android:layout_width="143dp"
        android:layout_height="34dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Turma"
        tools:layout_editor_absoluteX="233dp"
        tools:layout_editor_absoluteY="159dp" />

    <EditText
        android:id="@+id/et_dn_aluno"
        android:layout_width="160dp"
        android:layout_height="43dp"
        android:ems="10"
        android:inputType="date"
        android:text="Data Nascimento"
        tools:layout_editor_absoluteX="214dp"
        tools:layout_editor_absoluteY="354dp" />

    <EditText
        android:id="@+id/et_email_aluno"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="textEmailAddress"
        android:text="Email"
        tools:layout_editor_absoluteX="8dp"
        tools:layout_editor_absoluteY="299dp" />

    <EditText
        android:id="@+id/et_telefone_aluno"
        android:layout_width="215dp"
        android:layout_height="39dp"
        android:ems="10"
        android:inputType="phone"
        android:text="Telefone"
        tools:layout_editor_absoluteX="8dp"
        tools:layout_editor_absoluteY="250dp" />

    <EditText
        android:id="@+id/et_curso_aluno"
        android:layout_width="200dp"
        android:layout_height="43dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Curso"
        tools:layout_editor_absoluteX="8dp"
        tools:layout_editor_absoluteY="354dp" />

    <EditText
        android:id="@+id/et_endereco_aluno"
        android:layout_width="368dp"
        android:layout_height="37dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Endereço"
        tools:layout_editor_absoluteX="8dp"
        tools:layout_editor_absoluteY="62dp" />

    <EditText
        android:id="@+id/et_ne_aluno"
        android:layout_width="146dp"
        android:layout_height="38dp"
        android:ems="10"
        android:inputType="numberDecimal"
        android:text="Nota Entrada"
        tools:layout_editor_absoluteX="230dp"
        tools:layout_editor_absoluteY="299dp" />

    <Spinner
        android:id="@+id/s_cota_aluno"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="16dp"
        tools:layout_editor_absoluteX="62dp"
        tools:layout_editor_absoluteY="407dp" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="46dp"
        android:layout_height="38dp"
        android:text="Cota"
        android:textSize="19dp"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="407dp" />

    <EditText
        android:id="@+id/et_senha_aluno"
        android:layout_width="146dp"
        android:layout_height="32dp"
        android:ems="10"
        android:inputType="textPassword"
        tools:layout_editor_absoluteX="230dp"
        tools:layout_editor_absoluteY="407dp" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="61dp"
        android:layout_height="29dp"
        android:text="Senha"
        android:textSize="19dp"
        tools:layout_editor_absoluteX="162dp"
        tools:layout_editor_absoluteY="412dp" />

</android.support.constraint.ConstraintLayout>

<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.alessandro.tcc.CadastroFuncionarioActivity">

    <Button
        android:id="@+id/bt_cadastrar_funcionario"
        android:layout_width="368dp"
        android:layout_height="wrap_content"
        android:text="Cadastrar"
        tools:layout_editor_absoluteX="8dp"
        tools:layout_editor_absoluteY="453dp" />

    <EditText
        android:id="@+id/et_nome_funcionario"
        android:layout_width="368dp"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Nome"
        tools:layout_editor_absoluteX="8dp"
        tools:layout_editor_absoluteY="6dp" />

    <EditText
        android:id="@+id/et_endereco_funcionario"
        android:layout_width="368dp"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Endereço"
        tools:layout_editor_absoluteX="8dp"
        tools:layout_editor_absoluteY="50dp" />

    <EditText
        android:id="@+id/et_sexo_funcionario"
        android:layout_width="142dp"
        android:layout_height="37dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Sexo"
        tools:layout_editor_absoluteX="234dp"
        tools:layout_editor_absoluteY="250dp" />

    <EditText
        android:id="@+id/et_nacionalidade_funcionario"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Nacionalidade"
        tools:layout_editor_absoluteX="8dp"
        tools:layout_editor_absoluteY="196dp" />

    <EditText
        android:id="@+id/et_naturalidade_funcionario"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Naturalidade"
        tools:layout_editor_absoluteX="8dp"
        tools:layout_editor_absoluteY="250dp" />

    <EditText
        android:id="@+id/et_rg_funcionario"
        android:layout_width="198dp"
        android:layout_height="40dp"
        android:ems="10"
        android:inputType="text|number"
        android:text="RG"
        tools:layout_editor_absoluteX="8dp"
        tools:layout_editor_absoluteY="145dp" />

    <EditText
        android:id="@+id/et_cpf_funcionario"
        android:layout_width="160dp"
        android:layout_height="41dp"
        android:ems="10"
        android:inputType="number"
        android:text="CPF"
        tools:layout_editor_absoluteX="216dp"
        tools:layout_editor_absoluteY="146dp" />

    <EditText
        android:id="@+id/et_telefone_funcionario"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="phone"
        android:text="Telefone"
        tools:layout_editor_absoluteX="8dp"
        tools:layout_editor_absoluteY="301dp" />

    <EditText
        android:id="@+id/et_email_funcionario"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="textEmailAddress"
        android:text="Email"
        tools:layout_editor_absoluteX="8dp"
        tools:layout_editor_absoluteY="351dp" />

    <EditText
        android:id="@+id/et_cargo_funcionario"
        android:layout_width="368dp"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Cargo"
        tools:layout_editor_absoluteX="8dp"
        tools:layout_editor_absoluteY="100dp" />

    <EditText
        android:id="@+id/et_id_funcionario"
        android:layout_width="142dp"
        android:layout_height="37dp"
        android:ems="10"
        android:inputType="number"
        android:text="ID"
        tools:layout_editor_absoluteX="234dp"
        tools:layout_editor_absoluteY="196dp" />

    <EditText
        android:id="@+id/et_salario_funcionario"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="numberDecimal"
        android:text="Salário"
        tools:layout_editor_absoluteX="8dp"
        tools:layout_editor_absoluteY="398dp" />
</android.support.constraint.ConstraintLayout>

<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.alessandro.tcc">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".CadastroAlunoActivity"
            android:label="@string/title_activity_cadastro_aluno" />
        <activity
            android:name=".CadastroFuncionarioActivity"
            android:label="@string/title_activity_cadastro_professor" />
        <activity
            android:name=".CadastroLivroActivity"
            android:label="@string/title_activity_cadastro_livro" />
        <activity
            android:name=".CadastroDisciplinaActivity"
            android:label="@string/title_activity_cadastro_disciplina"></activity>
    </application>

</manifest>